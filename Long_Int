#import java.io.*;
class Long_Int {
  public static void main(String[] args) {

    
    long a = 2322331L;
    long b = 52341241L;
    int c = (int)a;
    int d = (int)b;
     System.out.println("Convert the number long(a) into int(c) :");  
    System.out.println(c);
     System.out.println("Convert the number long(b) into int(d) :");  // 2322331
    System.out.println(d);    // 52341241
  }
}
